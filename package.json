{
  "name": "kube-debug",
  "displayName": "kube-debug",
  "description": "debug your bin in pod",
  "version": "0.0.1",
  "repository": "https://github.com/ezxfv/kube-debug",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kube-debug.compileToPod",
        "title": "KubeDebug: Compile To Pod"
      },
      {
        "command": "kube-debug.attachToPod",
        "title": "KubeDebug: Attach To Pod"
      },
      {
        "command": "kube-debug.debugTest",
        "title": "KubeDebug: Debug test in Pod"
      }
    ]
  },
  "scripts": {
    "package": "pnpm vsce package --no-dependencies",
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vscode/test-electron": "^2.3.3",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^8.43.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "vscode-languageclient": "^8.1.0"
  }
}
